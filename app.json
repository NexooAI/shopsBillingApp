{
  "expo": {
    "name": "EasyBills",
    "slug": "mnv-billings",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/playstore-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/playstore-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a2e",
      "dark": {
        "backgroundColor": "#1a1a2e"
      },
      "light": {
        "backgroundColor": "#ffffff"
      },
      "hideExponentText": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.easybills.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/playstore-icon.png",
        "backgroundColor": "#1a1a2e"
      },
      "package": "com.easybills.app",
      "permissions": [
        "BLUETOOTH",
        "BLUETOOTH_ADMIN",
        "BLUETOOTH_CONNECT",
        "BLUETOOTH_SCAN",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#1a1a2e",
      "translucent": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-sqlite"
    ],
    "extra": {
      "eas": {
        "projectId": "14beefba-b017-4cbd-bfc4-0ea903c9a069"
      }
    },
    "owner": "mohandass9788"
  }
}